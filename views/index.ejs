<!DOCTYPE html>
<html>
<head>
	<title>Movie App</title>

	<% include head %>

</head>
<body>
	<div class="container" id="top">
		<% include navbar %>
		<h1><%=message %></h1>
		<!-- <div class="row"> -->
			<div class="col-sm-12">
				<% for(let i = 0; i < parsedData.length; i++){ %>
						<div class="col-sm-4">
							<a href="#title<%=i%>"><img src="<%= imageBaseUrl + parsedData[i].poster_path %>"/></a>
						</div>
				<% }; %>
				<% for(let j = 0; j < parsedData.length; j++){ %>
					<div id="title<%=j%>" class="row text">
						<h2><%= parsedData[j].title %>  </h2>
						<h4><%= parsedData[j].overview %></h4>
						<a href="#top">top</a>
					</div>
				<% } %>
			</div>
		<!-- </div> -->
	</div>

</body>
</html>